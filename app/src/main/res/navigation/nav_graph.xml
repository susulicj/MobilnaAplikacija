<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeProfileFragment">

    <fragment
        android:id="@+id/homeProfileFragment"
        android:name="com.example.projekatmobilne.Fragments.HomeProfileFragment"
        android:label="fragment_home_profile"
        tools:layout="@layout/fragment_home_profile" >
        <action
            android:id="@+id/action_homeProfileFragment_to_mapsFragment"
            app:destination="@id/mapsFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.example.projekatmobilne.Fragments.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps" >
        <action
            android:id="@+id/action_mapsFragment_to_addMarkerFragment"
            app:destination="@id/addMarkerFragment" />
    </fragment>
    <fragment
        android:id="@+id/addMarkerFragment"
        android:name="com.example.projekatmobilne.Fragments.AddMarkerFragment"
        android:label="fragment_add_marker"
        tools:layout="@layout/fragment_add_marker" />
</navigation>